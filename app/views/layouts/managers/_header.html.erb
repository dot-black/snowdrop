<!-- Main css styles -->
<%= stylesheet_link_tag    'manager', media: 'all', 'data-turbolinks-track' => true %>
<%= stylesheet_link_tag  "https://fonts.googleapis.com/icon?family=Material+Icons" %>

<!-- Main javascript files -->
<%= javascript_include_tag 'manager', 'data-turbolinks-track' => true %>
<%= csrf_meta_tags %>
<%= javascript_include_tag "https://cdn.jsdelivr.net/jquery.validation/1.15.0/jquery.validate.min.js" %>

<!-- Controller, action and locale spesific tag -->
<%= tag :meta, name: :page_spesific_javascript, action: action_name, controller: controller_name, locale: I18n.locale %>

