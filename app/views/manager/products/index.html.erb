<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">

    <h2><%= t "manager.layout.#{current_page_name}" %></h2>

    <ol class="breadcrumb">
      <li><%= t "manager.layout.#{current_page_name}" %></li>
      <li><b><%= t 'manager.layout.list' %></b></li>
    </ol>
  </div>
  <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
    <h2><%= link_to t('manager.layout.new'), new_manager_product_path, class: "btn btn-primary pull-right" %></h2>
  </div>
</div>

<div class="tabs-container">
  <div class="manager-products-filter">
    <%= render "manager_products_filter" %>
  </div>
  <div class="products-list">
    <%= render "list" %>
  </div>
</div>
